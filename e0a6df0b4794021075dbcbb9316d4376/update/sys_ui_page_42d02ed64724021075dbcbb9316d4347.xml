    data.projectName = gr.getValue('short_description');
    data.projectManager = gr.getDisplayValue('project_manager');
    data.portfolio = gr.getDisplayValue('primary_portfolio');
    data.approvedStartDate = gr.getDisplayValue('approved_start_date');
    data.approvedEndDate = gr.getDisplayValue('approved_end_date');
    data.plannedStartDate = gr.getDisplayValue('start_date');
    data.plannedEndDate = gr.getDisplayValue('end_date');
	data.state = gr.getDisplayValue('state');
	data.phase = gr.getDisplayValue('phase');
	data.percentComplete = gr.getValue('percent_complete');
	data.actualCost = gr.getDisplayValue('work_cost');
	data.actualStartDate = gr.getDisplayValue('work_start');
	data.actualEndDate = gr.getDisplayValue('work_end');
	data.plannedCost = gr.getDisplayValue('cost');
}

</g:evaluate>

